@using EventsWebsite.Models
@model List<UserModel> 

@{

    ViewBag.Title = "Welkom, Admin";
}

<div class="intro-header">
    <h1>@ViewBag.Title</h1>
</div>
<div class="container">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Voornaam</th>
                <th>Achternaam</th>
                <th>Gebruikersnaam</th>
            </tr>
        </thead>
        <tbody>
            
                @foreach (UserModel u in Model)
                {
                    <tr>
                        <td><a href="@Url.Action("EditProfileAsAdmin","User", u)">@u.Voornaam</td>
                        <td>@u.Achternaam</td>
                        <td>@u.Username</td>
                    </tr>
                }
                    
        </tbody>
    </table>

</div>